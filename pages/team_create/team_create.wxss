
/* pages/team_create/team_create.wxss */
page{
  background-color: #fff;
}

.head{
  background-color: #fff;
  padding-top: 50rpx;
  border-bottom: solid 1px #f19d38;
}

.class_name{
  font-size: 40rpx;
  font-weight: bold;
  width:750rpx;
  margin-left: 30rpx;
  color: #333;
}

.intro{
  margin-left: 30rpx;
  margin-top: 14rpx;
  height: 50rpx;
  width: 690rpx;
}

.intro  navigator{
  text-decoration: underline;
  color: #000080;
  font-size: 28rpx;   
  height: 50rpx;
  line-height: 50rpx;
  float: left;
}

.intro text{
  font-size: 28rpx;   
  height: 50rpx;
  line-height: 50rpx;
  color: #444;
  float: left;
}

.intro image{
  width: 50rpx;
  height: 50rpx;
  float: left;
}

.sup{
  margin-left: 12rpx;
}

.count{
  margin-left: 110rpx;
}

.out{
  margin-left: 110rpx;
}

.option{
  width: 375rpx;
  height: 80rpx;
  line-height: 80rpx;
  float: left;
  text-align: center;
  margin-top: 40rpx;
  font-size: 30rpx;
}

.choose{
  border-top: solid 5rpx #f19d38;
  background-color: #f19e3817;
}

.create_box{
  width:730rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
  border: solid 1rpx #f19d38;
  border-radius: 10rpx;
  background-color: #fff;
}

.id{
  margin-left: 10rpx;
  margin-top: 20rpx;
  float: left;
  font-size: 30rpx;
}

.mem_sup{
  margin-left: 105rpx;
  margin-top: 20rpx;
  float: left;
  font-size: 30rpx;
}

.leader{
  margin-left: 105rpx;
  margin-top: 20rpx;
  float: left;
  font-size: 30rpx;
}

.leader text{
  float: left;
}

.leader input{
  width: 178rpx;
  height: 28rpx;
  font-size: 25rpx;
  border: solid 1rpx #D0D0D0;
  margin-left: 10rpx;
  float: left;
  border-radius: 4rpx;
}

.more{
  color: #000000;
  margin-left: 10rpx;
  margin-top: 14rpx;
  font-size: 30rpx;
}

.more input{
  width:710rpx;
  height: 180rpx;
  border: solid 1rpx #D0D0D0;
  border-radius: 4rpx;
  margin-top: 14rpx;
  font-size: 30rpx;
}

.invite{
  margin-top: 30rpx;
  margin-left: 10rpx;
  width: 710rpx;
  font-size: 30rpx;
}

.single_list{
  border-top: solid 1px #f19d38;
  margin-top: 5rpx;
  width: 710rpx;
}

.checkbox{
  margin-left: 35rpx;
  margin-top: 20rpx;
  color: #696969;
  width: 100rpx;
  height: 150rpx;
  float: left;
  font-size: 25rpx;
  text-align: center;
  position: relative;
}

.checkbox image{
  height: 100rpx;
  width: 100rpx;
  border-radius: 6rpx;
}

.checkbox checkbox{
  position: absolute;
  left: 0rpx;
  top: 0rpx;
}

.commit{
  width:150rpx;
  height: 60rpx;
  font-size:30rpx;
  background-color: #f19d38;
  line-height: 60rpx;
  margin-top: 50rpx;
  margin-right: 10rpx;
  margin-bottom: 30rpx;
}
